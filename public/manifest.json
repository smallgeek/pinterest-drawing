{
  "name": "Pinterest Drawing",
  "manifest_version": 2,
  "version": "1.0",
  "permissions": [
    "https://*.pinterest.jp/",
    "storage"
  ],
  "browser_action": {
    "default_icon": "128.png",
    "default_title": "Pinterest Drawing",
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["https://*.pinterest.jp/*"],
      "css": ["css/app.css"],
      "js": [
        "js/chunk-vendors.js",
        "js/app.js"
      ]
    }
  ],
  "icons": {
    "128": "128.png"
  }
}
