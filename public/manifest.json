{
  "name": "Pinterest Slideshow",
  "manifest_version": 2,
  "version": "1.0",
  "permissions": [
    "https://*.pinterest.jp/"
  ],
  "browser_action": {
    "default_icon": "128.png",
    "default_title": "Pinterest Slideshow"
  },
  "content_scripts": [
    {
      "matches": ["https://*.pinterest.jp/*"],
      "css": ["style.css"],
      "js": ["content_scripts.js"]
    }
  ],
  "icons": {
    "128": "128.png"
  }
}
